programa {
  funcao inicio() {
  
   
        cadeia senha

       
        enquanto (verdadeiro)
        {
            escreva("Digite a senha: ")
            leia(senha)

           
            se (senha == "12345")
            {
                escreva("Senha correta! Acesso permitido.\n")
                pare 
            }
            senao
            {
                escreva("Senha incorreta! Tente novamente.\n")
            }
        }
  }
}
